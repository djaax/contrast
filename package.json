{
  "name": "@smockle/contrast",
  "version": "4.0.7",
  "description": "Analyse luminosity contrast ratio",
  "main": "dist/lib/index",
  "types": "dist/lib/index.mjs.d.ts",
  "bin": {
    "contrast": "dist/bin/index.js"
  },
  "scripts": {
    "build": "npm run build:mjs && npm run build:js",
    "build:js": "echo \"Not building .js files\" && exit 0",
    "build:mjs": "tsc -d && bash scripts/rename.sh",
    "start": "node -r @std/esm dist/bin/index.mjs",
    "debug": "node --nolazy --inspect-brk=5858 -r @std/esm dist/bin/index.mjs",
    "test": "node -r @std/esm test/index.mjs | colortape",
    "coverage": "nyc --require @std/esm --extension .mjs --reporter=lcov node test/index.mjs | colortape",
    "postcoverage": "nyc report",
    "prepublishOnly": "npm test",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smockle/contrast.git"
  },
  "author": "Clay Miller <clay@smockle.com> (https://smockle.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/smockle/contrast/issues"
  },
  "homepage": "https://github.com/smockle/contrast#readme",
  "devDependencies": {
    "@types/node": "^9.4.7",
    "codecov": "^3.0.0",
    "colortape": "^0.1.2",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "nyc": "^11.0.2",
    "typescript": "^2.8.0-dev.20180314",
    "prettier-eslint-cli": "^4.3.0"
  },
  "lint-staged": {
    "*.{js,mjs}": [
      "./node_modules/.bin/prettier-eslint --write",
      "git add"
    ]
  },
  "@std/esm": {
    "cjs": true
  },
  "dependencies": {
    "@std/esm": "^0.25.0"
  }
}
